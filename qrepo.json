{
	"name": "Offion",
	"author": "Nathanne Isip",
	"version": "v0.0.1",
	"description": "Debian-based personal browser built with Electron framework, integrating Tor for enhanced privacy and anonymous browsing.",
	"git": "https://github.com/nthnn/offion.git",
	"scripts": {
		"clean": ["rm -rf .parcel-cache dist"],
		"install": [
			"rm -rf node_modules",
			"npm install"
		],
		"build": [
			"qrepo run clean",
			"npx parcel build index.pug --dist-dir dist --public-url . --no-source-maps",
			"cp index.js dist/launch.js",
			"mkdir -p dist/assets",
			"cp assets/*.gif dist/assets/"
		],
		"dev": [
			"qrepo run build",
			"npx electron dist/launch.js"
		]
	}
}